{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Parameters": {
    "postgres-admin": "postgres",
    "postgres-password": "postgres"
  },
  "DependsOnOptions": {
    "Retry": {
      "MaxRetryAttempts": 100,
      "BackoffType": 1,
      "Delay": "00:00:3",
      "MaxDelay": "00:00:10"
    },
    "Timeout":{
      "Timeout": "00:00:30"
    }
  }
}
